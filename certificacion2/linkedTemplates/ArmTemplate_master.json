{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "certificacion2"
		},
		"Certifiacion3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Certifiacion3'"
		},
		"Certificacion2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Certificacion2'"
		},
		"Certificacion2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://certificacion2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Certifiacion3_connectionString": {
						"value": "[parameters('Certifiacion3_connectionString')]"
					},
					"Certificacion2_accountKey": {
						"value": "[parameters('Certificacion2_accountKey')]"
					},
					"Certificacion2_properties_typeProperties_url": {
						"value": "[parameters('Certificacion2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}